---
import Layout from "../../../layouts/Layout.astro";

import img from "../../..//assets/images/services/s-plomberie-01.jpg";
import {
  seoPlaces,
  seo_keys,
} from "../../../data/seoPlomberieUrgenceKeywordsVillesKey";

import ctaBg from "../../../assets/images/cta-v1-bg.jpg";
import img1 from "../../../assets/images/ressources/pourquoi-nous-choisir-01.jpg";
import img2 from "../../../assets/images/ressources/pourquoi-nous-choisir-02.jpg";
import img3 from "../../../assets/images/ressources/pourquoi-nous-choisir-03.jpg";
import img4 from "../../../assets/images/ressources/pourquoi-nous-choisir-04.jpg";


export async function getStaticPaths() {
  const paths = [];

  for (const keySlug of Object.keys(seo_keys)) {
    for (const placeSlug of Object.keys(seoPlaces)) {
      paths.push({ params: { keySlug, placeSlug } });
    }
  }

  return paths;
}

const { params } = Astro;
const { keySlug, placeSlug } = params;

const key = keySlug as keyof typeof seo_keys;
const place = placeSlug as keyof typeof seoPlaces;

const keyLabel = seo_keys[key] || keySlug;
const placeLabel = seoPlaces[place] || placeSlug;

// Page Header
const header = {
  title1: `${keyLabel} ${placeLabel}`,
  title2: "",
};
// Page Header
